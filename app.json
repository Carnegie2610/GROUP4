{
  "expo": {
    "name": "milko",
    "slug": "milko",
    "scheme": "savewaka", 
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./fonts/PlaywriteDEGrund-VariableFont_wght.ttf"]
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.milkorg.savewaka",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.milkorg.safewaka"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
